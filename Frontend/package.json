{
  "name": "gbf-frontend",
  "version": "2.1.0",
  "description": "Granblue Party Frontend",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production node server.js",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack.server.config.js",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack.client.config.js",
    "build:static": "cross-env NODE_ENV=production webpack --config webpack.static.config.js",
    "build": "npm run build:server && npm run build:client && npm run build:static",
    "dev:server": "webpack -w --config webpack.server.config.js",
    "dev:client": "webpack server --config webpack.client.config.js",
    "dev:static": "webpack --config webpack.static.config.js",
    "nodemon": "nodemon --trace-warnings server.js"
  },
  "author": {
    "name": "Minimalist",
    "email": "contact@granblue.party",
    "url": "https://www.granblue.party"
  },
  "license": "GPL-3.0",
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "copy-webpack-plugin": "^13.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "cssnano": "^7.0.7",
    "extract-css-chunks-webpack-plugin": "^4.10.0",
    "html-webpack-plugin": "^5.6.4",
    "ifdef-loader": "^2.3.2",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "postcss-loader": "^8.1.1",
    "sitemap-webpack-plugin": "^1.1.1",
    "url": "^0.11.4",
    "vue-loader": "^15.11.1",
    "webpack": "^5.99.9",
    "webpack-cdn-plugin": "^3.3.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "latest-2",
    "axios": "1.10.0",
    "browserslist": "^4.26.3",
    "cookie-parser": "^1.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "remove-files-webpack-plugin": "^1.5.0",
    "tailwindcss": "^3.4.18",
    "vue": "^2.7.16",
    "vue-router": "^3.6.5",
    "vue-server-renderer": "^2.7.16",
    "vuex": "^3.6.2"
  },
  "packageManager": "yarn@4.1.1"
}
